<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:text="output" android:id="@+id/output"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginLeft="50px" android:layout_marginTop="50px"
		android:textSize="20px"></TextView>
	<TextView android:text="coord" android:id="@+id/coord"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginLeft="50px" android:layout_marginTop="50px"
		android:textSize="20px"></TextView>
	<ToggleButton android:layout_gravity="right"
		android:checked="false" android:textSize="20px" android:textOn="connect"
		android:textOff="disconnect" android:id="@+id/connect"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></ToggleButton>
</LinearLayout>
